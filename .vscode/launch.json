    "version": "0.0.1",
    "configurations": [
        {
            "name": "Recommendations playlist",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/bin/recommendations_playlist.py",
            "args": [
                "${workspaceFolder}/bin/example.recommendations.config",
                "${workspaceFolder}/../lastipy_config/.keys"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "New releases playlist",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/bin/save_new_releases.py",
            "args": [
                "sonofjack3",
                "${workspaceFolder}/../lastipy_config/.keys"
            ],
            "console": "integratedTerminal" 
        }
    ]
}